{
  "name": "curve-solver",
  "version": "1.0.0",
  "description": "Curve Fitting GitHub Page",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "deploy": "pnpm run build && gh-pages -d dist",
    "format": "prettier --write src/**/*.{js,ts,vue,html,css,md}",
    "format:check": "prettier --check src/**/*.{js,ts,vue,html,css,md}",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "mathjs": "^14.5.2",
    "ml-levenberg-marquardt": "^4.1.3",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "gh-pages": "^6.2.0",
    "prettier": "^3.4.2",
    "typescript": "~5.6.0",
    "vite": "^6.0.7",
    "vue-tsc": "^2.2.0"
  },
  "packageManager": "pnpm@10.11.1"
}
